!----------------------------------------------
!	file: .Xdefaults
!	author: Eclectric Sheep, based on file by Chris Borrrell
!	modified: 03-08-10
!	vim:enc=utf-8:nu:ai:si:et:ts=4:sw=4:ft=xdefaults:
!----------------------------------------------

Xft.dpi:		    96
Xft.antialias:		true
Xft.hinting:		full
Xft.hintstyle:		hintslight
Xft.rgba:		    rgb

URxvt.title:        shell
! URxvt.transparent:	false
! URxvt.inheritPixmap:false
URxvt.scrollBar:	false
URxvt.saveLines:	32767
URxvt.cursorBlink:	true
! URxvt.cursorColor:	#00E0FF
URxvt.color:		#121212
URxvt.shading:		30
! URxvt.tintcolor:	#000000
URxvt*depth:		32
URxvt.visualBell:	false
URxvt.fading:       25
URxvt.buffered:     true

URxvt.urgentOnBell: True
URxvt.font:		        xft:Inconsolata:pixelsize=15:antialias=true:hinting=true
URxvt.boldFont:		    xft:Inconsolata:pixelsize=15:antialias=true:hinting=true:bold
URxvt.italicFont:	    xft:Inconsolata:pixelsize=15:antialias=true:hinting=true:italic
URxvt.bolditalicFont:	xft:Inconsolata:pixelsize=15:antialias=true:hinting=true:bold:italic

URxvt.perl-ext-common:  default,matcher,clipboard,tabbed
URxvt.urlLauncher:      /usr/bin/jumanji
URxvt.matcher.button:   1


URxvt*background: [90]#121212
URxvt*foreground: #00e0ff
URxvt*mousePointerColor: #FFFFFF

URxvt.tabbed.tabbar-fg: 7
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    2
URxvt.tabbed.tab-bg:    0

